pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git branch: 'main', url: 'https://github.com/saks786/jenkins.git'
            }
        }
        stage('Build') {
            steps {
                sh 'echo Building project...'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo Deploying project...'
            }
        }
    }
}
